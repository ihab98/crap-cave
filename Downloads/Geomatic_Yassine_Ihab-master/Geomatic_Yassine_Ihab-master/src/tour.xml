	<!-- startup action - load the first scene -->
	<action name="startup" autorun="onstart">
		if(startscene === null OR !scene[get(startscene)], copy(startscene,scene[0].name); );
		loadscene(get(startscene), null, MERGE);
		if(startactions !== null, startactions() );
	</action>

	<action name="nextscene">
		if(%1 != findnext, set(i,0));
		txtadd(scenexml,'<krpano>',get(scene[%i].content),'</krpano>');
		if(scenexml == xml.content,
	   	inc(i);
	   	if(i == scene.count, set(i,0));
	   	loadscene(get(scene[%i].name), null, KEEPVIEW, BLEND(0.2));
	  	,
	   	inc(i);
	   	if(i LT scene.count, nextscene(findnext));
	  	);
	</action>

	<action name="prevscene">
		if(%1 != findnext, sub(i,scene.count,1));
		txtadd(scenexml,'<krpano>',get(scene[%i].content),'</krpano>');
		if(scenexml == xml.content,
	   	dec(i);
	   	if(i LT 0, sub(i,scene.count,1));
	   	loadscene(get(scene[%i].name), null, KEEPVIEW, BLEND(0.2));
	  	,
	   	dec(i);
	   	if(i GE 0, prevscene(findnext));
	  	);
	</action>

	<action name="draglayer">
		copy(drag_currentx, x);
		copy(drag_currenty, y);
		copy(drag_stagex, mouse.stagex);
		copy(drag_stagey, mouse.stagey);
		indexoftxt(align_contains_right, get(align), 'right');
		indexoftxt(align_contains_bottom, get(align), 'bottom');
		calc(drag_align_x, align_contains_right GE 0 ? -1 : +1);
		calc(drag_align_y, align_contains_bottom GE 0 ? -1 : +1);
		asyncloop(pressed,
			calc(x, drag_currentx + (mouse.stagex - drag_stagex)*drag_align_x);
			calc(y, drag_currenty + (mouse.stagey - drag_stagey)*drag_align_y);
		  );
	</action>

	<action name="extract_first_angle_js" type="Javascript"><![CDATA[
		
		var mx = krpano.get("mouse.downx");
		var my = krpano.get("mouse.downy");
		var pnt = krpano.screentosphere(mx,my);
		var h = pnt.x;
		var v = pnt.y;
		var adj_h = - (h - 90);
		krpano.set("data[first_angle].angle", adj_h );
		krpano.set("data[first_angle].height", v );

		krpano.call("removehotspot('temp_hs')");

	      var hs_name = "temp_hs";

	      krpano.call("addhotspot(" + hs_name + ")");
	      krpano.set("hotspot["+hs_name+"].url", "tracked.png");
	      krpano.set("hotspot["+hs_name+"].ath", h);
	      krpano.set("hotspot["+hs_name+"].atv", v);
	      krpano.set("hotspot["+hs_name+"].distorted", true);
	      krpano.set("hotspot["+hs_name+"].alpha", 0.6);

		alert('First perspective selected: ' + adj_h);
		]]>
			
		</action>

	<action name="extract_second_angle_js" type="Javascript"><![CDATA[

		var mx = krpano.get("mouse.downx");
		var my = krpano.get("mouse.downy");
		var pnt = krpano.screentosphere(mx,my);
		var h = pnt.x;
		var v = pnt.y;
		var adj_h = - (h - 90);
		krpano.set("data[second_angle].angle", adj_h );
		krpano.set("data[second_angle].height", v );

		krpano.call("removehotspot('temp_hs')");

	      var hs_name = "temp_hs";

	      krpano.call("addhotspot(" + hs_name + ")");
	      krpano.set("hotspot["+hs_name+"].url", "tracked.png");
	      krpano.set("hotspot["+hs_name+"].ath", h);
	      krpano.set("hotspot["+hs_name+"].atv", v);
	      krpano.set("hotspot["+hs_name+"].distorted", true);
	      krpano.set("hotspot["+hs_name+"].alpha", 0.6);

		alert('Second perspective selected: ' + adj_h);
		]]>
			
		</action>

	<action name="compute_coordinates" type="Javascript"><![CDATA[
		
		//alert(krpano.get('data[current_coordinates].lat') + ' ; ' + krpano.get('data[current_coordinates].lng'));

        var corax = krpano.get('data[current_coordinates].lat');
        var coray = krpano.get('data[current_coordinates].lng');



        var corbx = krpano.get('data[last_coordinates].lat');
        var corby = krpano.get('data[last_coordinates].lng');

        //alert('current coordinates A and B: ' + corax + '/' + coray + ' ; ' + corbx + '/' + corby);

        var alpha = krpano.get('data[first_angle].angle');
        var beta = krpano.get('data[second_angle].angle');

        //alert('angles alpha and beta: ' + alpha + ' ; ' + beta);

        var xa = Math.cos(alpha * Math.PI/180);
        var ya = Math.sin(alpha * Math.PI/180);

        var xb = Math.cos(beta * Math.PI/180);
        var yb = Math.sin(beta * Math.PI/180);

        //alert('vectors A and B: ' + xa + '/' + ya + " ; " + xb + '/' + yb);

        var pax = parseFloat(corax) + parseFloat(xa);
        var pay = parseFloat(coray) + parseFloat(ya);

        var pbx = parseFloat(corbx) + parseFloat(xb);
        var pby = parseFloat(corby) + parseFloat(yb);

        //alert('increment points A and B: ' + pax + '/' + pay + ' ; ' + pbx + '/' + pby);

        var u = -((corax - pax) * (coray - corby) - (coray - pay) * (corax - corbx))/((corax - pax)*(corby - pby) - (coray - pay)*(corbx - pbx));

        var intersect_x = parseFloat(corax) + parseFloat(u*(pax - corax));
        var intersect_y = parseFloat(coray) + parseFloat(u*(pay - coray));

        //alert('Determinant u: ' + u);

        krpano.set('data[intersection].x', intersect_x);
        krpano.set('data[intersection].y', intersect_y);

        alert("Object coordinates are lat=" + krpano.get('data[intersection].x') + " and lng=" + krpano.get('data[intersection].y') );

        krpano.call('layer[map].addspot("new_spot","get(data[intersection].x)", "get(data[intersection].y)", 0)');
       	krpano.call('layer[map].activatespot("new_spot")');
       	krpano.call('layer[map].pantospot("new_spot")');

       	window.open("objectform.html","width=100,height=60");


  
	]]></action>

	<action name="update_coordinates" type="Javascript" args="lat,lng"><![CDATA[

		var cur_lat = krpano.get('data[current_coordinates].lat');
		var cur_lng = krpano.get('data[current_coordinates].lng');

		krpano.set('data[last_coordinates].lat', cur_lat);
		krpano.set('data[last_coordinates].lng', cur_lng);

		var new_lat = args[1];
		var new_lng = args[2];

		krpano.set('data[current_coordinates].lat', new_lat);
		krpano.set('data[current_coordinates].lng', new_lng);


	]]></action>

